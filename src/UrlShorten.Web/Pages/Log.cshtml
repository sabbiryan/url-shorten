@page
@model LogModel
@{
    ViewData["Title"] = "View Log";
}

<h4>
    View Logs
</h4>


<div class="row">

    <div class="col-sm-12" style="max-height: 600px;overflow: auto;">
        @if (string.IsNullOrWhiteSpace(Model.Message))
        {
            foreach (var log in Model.Logs)
            {
                <span>@log</span>
            }
        }
        else
        {
            <p>@Model.Message</p>
        }
    </div>

</div>
